#review-wod.page
  .page-header
    .toolbar
      %div
        %a.awesome.icon-chevron-left.goback{href: '#'}
      %h1= @entry.wodName()
  .content-main
    .content-block
      .yellow-block
        .review-score
          .score= @entry.scoreString()
            %span.score-level= @entry.type
          %div.score-tags
            %span.pr Personal Record
          %p.score-details= @entry.details

      %a.button.bottom.fade{href: "#home"} Jump To Dashboard

  != JST['superfit/views/_history'](wod: @wod, pastEntries: @pastEntries)

  .footer
    %a.button.bottom.lighter.dissolve{"data-id": @entry.id, href: "#edit-wod"} Edit Workout
    %a.button.bottom.red.pop.delete{"data-id": @entry.id, href: "#home"} Delete Workout
